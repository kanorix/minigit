{
    "java.configuration.updateBuildConfiguration": "automatic",
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "files.exclude": {
        ".gitattributes": true,
        ".gradle": true,
        "gradle": true,
        "gradlew": true,
        "gradlew.bat": true,
        "app/bin/**": true,
        "app/build/**": true,
    },
    "java.format.settings.url": ".vscode/java-formatter.xml",
    "java.format.settings.profile": "GoogleStyle"
}
