{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "auto rebuild",
			"type": "shell",
			"command": "./gradlew -t build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
		{
			"label": "build",
			"type": "shell",
			"command": "./gradlew build"
		},
		{
			"label": "clean",
			"type": "shell",
			"command": "./gradlew clean"
		}
	]
}
